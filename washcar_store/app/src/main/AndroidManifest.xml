<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.phoneandhisfriends.washcar_store">

    <application
        android:allowBackup="true"
        android:icon="@drawable/store_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/store_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".DetailCustomerActivity"></activity>
        <activity android:name=".HistoryListActivity">

        </activity>
    </application>

</manifest>